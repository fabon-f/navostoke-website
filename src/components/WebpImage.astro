---
import type { HTMLAttributes } from 'astro/types';
export type Props = HTMLAttributes<'img'> & {
  src: string;
};

const props = Astro.props;
const { src, width, height } = props;
---
<picture><source srcset={`${src}.webp`} width={width} height={height} type="image/webp" /><source srcset={`${src}.png`} width={width} height={height} type="image/png" /><img {...props} src={`${src}.png`} /></picture>
